<div>
    <h1 class="card z-depth-3 center-align"> Check out the latest from the Weather Channel</h1>
    <div class="row">
        <div class="col s3"></div>
        <h5 class="card z-depth-3 center-align col s6">Click 'Scrape' to get the scoope</h5>
    </div>


</div>

<div class="row">
    <div class="col s2"></div>
    <div class="col s8 left-align">
        {{#each article}}
        <div class="card-panel">
            <h3>{{this.title}}</h3>
            <a href="/readArticle/{{this._id}}" class="waves-effect waves-light btn" id="SignUpbtn"> Read Weather
                Article</a>
        </div>

        {{/each}}
    </div>
</div>